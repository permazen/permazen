<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="io.permazen.cli.jshell.PermazenJShellShellSession"/>
        <Method name="removeIfTemporary"/>
        <Bug pattern="RV_RETURN_VALUE_IGNORED_BAD_PRACTICE"/>
    </Match>
    <Match>
        <Class name="io.permazen.cli.jshell.PermazenExecutionControlProvider"/>
        <Method name="createMemoryClassLoader"/>
        <Bug pattern="DP_CREATE_CLASSLOADER_INSIDE_DO_PRIVILEGED"/>
    </Match>
</FindBugsFilter>
