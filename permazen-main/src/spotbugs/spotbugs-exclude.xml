<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,CT_CONSTRUCTOR_THROW"/>
    </Match>
    <Match>
        <Class name="io.permazen.Util"/>
        <Method name="newParameterizedType"/>
        <Bug pattern="LI_LAZY_INIT_UPDATE_STATIC"/>
    </Match>
    <Match>
        <Class name="io.permazen.PermazenCollectionField"/>
        <Method name="importPlain"/>
        <Bug pattern="DE_MIGHT_IGNORE,REC_CATCH_EXCEPTION"/>
    </Match>
    <Match>
        <Class name="io.permazen.PermazenMapField"/>
        <Method name="importPlain"/>
        <Bug pattern="DE_MIGHT_IGNORE,REC_CATCH_EXCEPTION"/>
    </Match>
    <Match>
        <Class name="io.permazen.Permazen"/>
        <Method name="&lt;init&gt;"/>
        <Bug pattern="DP_CREATE_CLASSLOADER_INSIDE_DO_PRIVILEGED,REC_CATCH_EXCEPTION"/>
    </Match>
    <Match>
        <Class name="io.permazen.OnDeleteScanner$DeleteMethodInfo"/>
        <Bug pattern="EQ_DOESNT_OVERRIDE_EQUALS"/>
    </Match>
    <Match>
        <Class name="io.permazen.OnCreateScanner$CreateMethodInfo"/>
        <Bug pattern="EQ_DOESNT_OVERRIDE_EQUALS"/>
    </Match>
</FindBugsFilter>
