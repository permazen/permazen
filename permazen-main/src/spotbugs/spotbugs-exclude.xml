<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="io.permazen.Util"/>
        <Method name="newParameterizedType"/>
        <Bug pattern="LI_LAZY_INIT_UPDATE_STATIC"/>
    </Match>
    <Match>
        <Class name="io.permazen.OnVersionChangeScanner"/>
        <Bug pattern="SE_COMPARATOR_SHOULD_BE_SERIALIZABLE"/>
    </Match>
    <Match>
        <Class name="io.permazen.JCollectionField"/>
        <Method name="importPlain"/>
        <Bug pattern="DE_MIGHT_IGNORE,REC_CATCH_EXCEPTION"/>
    </Match>
    <Match>
        <Class name="io.permazen.JMapField"/>
        <Method name="importPlain"/>
        <Bug pattern="DE_MIGHT_IGNORE,REC_CATCH_EXCEPTION"/>
    </Match>
</FindBugsFilter>
