<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,CT_CONSTRUCTOR_THROW"/>
    </Match>
    <Match>
        <Class name="io.permazen.kv.sql.SQLKVTransaction"/>
        <Method name="query"/>
        <Bug pattern="OBL_UNSATISFIED_OBLIGATION"/>
    </Match>
</FindBugsFilter>
