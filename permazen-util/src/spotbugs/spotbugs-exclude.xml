<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="io.permazen.util.AbstractXMLStreaming"/>
        <Method name="getBooleanAttr"/>
        <Bug pattern="NP_BOOLEAN_RETURN_NULL"/>
    </Match>
    <Match>
        <Class name="io.permazen.util.ByteUtil$1"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.util.MapEntryConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.util.ConvertedMapEntry"/>
        <Bug pattern="EQ_DOESNT_OVERRIDE_EQUALS"/>
    </Match>
    <Match>
        <Class name="io.permazen.util.CloseableTracker"/>
        <Method name="&lt;init&gt;"/>
        <Bug pattern="ESync_EMPTY_SYNC"/>
    </Match>
</FindBugsFilter>
