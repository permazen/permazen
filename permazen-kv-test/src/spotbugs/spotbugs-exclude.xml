    <Match>
        <Class name="io.permazen.kv.test.KVDatabaseTest"/>
        <Method name="testMultipleThreadsTransaction"/>
        <Bug pattern="RU_INVOKE_RUN"/>
    </Match>
</FindBugsFilter>
