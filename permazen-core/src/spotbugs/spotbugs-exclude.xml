<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="io.permazen.schema.SchemaObjectType"/>
        <Method name="differencesFrom"/>
        <Local name="thisField"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.encoding.FileEncoding$FileConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.encoding.PatternEncoding$PatternConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.encoding.URIEncoding$URIConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.encoding.InternetAddressEncoding$InternetAddressConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.encoding.ZoneIdEncoding$ZoneIdConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.util.ObjIdMap$Entry"/>
        <Bug pattern="EQ_DOESNT_OVERRIDE_EQUALS"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.util.ObjIdMap"/>
        <Field name="modcount"/>
        <Bug pattern="VO_VOLATILE_INCREMENT"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.util.ObjIdBiMultiMap"/>
        <Method name="clone"/>
        <Bug pattern="ML_SYNC_ON_UPDATED_FIELD"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.Transaction"/>
        <!-- <Method name="hasFieldMonitor"/> -->
        <Field name="monitorMap"/>
        <Bug pattern="IS_FIELD_NOT_GUARDED"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.Transaction"/>
        <!-- <Method name="hasFieldMonitor"/> -->
        <Field name="hasFieldMonitorCache"/>
        <Bug pattern="IS_FIELD_NOT_GUARDED"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.Transaction"/>
        <!-- <Method name="doCopyFields, changeVersion"/> -->
        <Field name="versionChangeListeners"/>
        <Bug pattern="IS_FIELD_NOT_GUARDED"/>
    </Match>
    <Match>
        <Class name="io.permazen.core.encoding.StringConvertedEncoding"/>
        <Method name="write"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
    </Match>
</FindBugsFilter>
