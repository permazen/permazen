<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,CT_CONSTRUCTOR_THROW"/>
    </Match>
    <Match>
        <Class name="io.permazen.encoding.FileEncoding$FileConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.encoding.PatternEncoding$PatternConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.encoding.URIEncoding$URIConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.encoding.InternetAddressEncoding$InternetAddressConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.encoding.ZoneIdEncoding$ZoneIdConverter"/>
        <Bug pattern="HE_INHERITS_EQUALS_USE_HASHCODE"/>
    </Match>
    <Match>
        <Class name="io.permazen.encoding.StringConvertedEncoding"/>
        <Method name="write"/>
        <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
    </Match>
</FindBugsFilter>
